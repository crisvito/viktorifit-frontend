{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not chrome <= 109",
    "not edge <= 133",
    "not opera <= 114",
    "not samsung <= 27",
    "not ios_saf <= 18.3",
    "not safari <= 18.3"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "scrollbar-width",
          "scrollbar-color"
        ]
      }
    ],
    "css-prefix-order": "off",
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ]
  }
}